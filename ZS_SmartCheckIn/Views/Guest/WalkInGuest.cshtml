        function Validate() {
            var flag = true;
            var visa = true;
            var card = true;
            $(".requiredv").html("");
            var FirstName = $("#FirstName").val();
            var LastName = $("#LastName").val();
            var Address = $("#Address").val();
            var Nationality = $("#Nationality").val();
            var DOB = $("#DOB").val();

            if (FirstName == null || FirstName.trim() == "") {
                $("#validFName").html("This field is required.");
                flag = false; card = false;
            }
            if (LastName == null || LastName.trim() == "") {
                $("#validLName").html("This field is required.");
                flag = false; card = false;
            }
            //if (Address == null || Address.trim() == "") {
            //    $("#validAddress").html("This field is required.");
            //    flag = false;
            //}
            if (Nationality == null || Nationality.trim() == "") {
                $("#validNationality").html("This field is required.");
                flag = false; card = false;
            }
            if (DOB != '' && DOB != null) {
                if (isDate(DOB) == false) {
                    $("#validDOB").html("Enter Valid Date Of Birth.");
                    flag = false; card = false;
                }
            }

            if (Nationality != "IND" && Nationality != null) {
                var today = new Date();
                var dd = today.getDate();
                var mm = today.getMonth() + 1;
                var yyyy = today.getFullYear();
                if (dd < 10) {
                    dd = '0' + dd;
                }
                if (mm < 10) {
                    mm = '0' + mm;
                }
                var Today = dd + '/' + mm + '/' + yyyy;

                var DocumentNo = $("#DocumentNo").val();
                //var CountryofIssue = $("#CountryofIssue").val();
                var DateOfIssue = $("#DateOfIssue").val();
                var ExpiryDate = $("#ExpiryDate").val();
                var regExp = /(\d{1,2})\/(\d{1,2})\/(\d{2,4})/;

                if (DocumentNo == null || DocumentNo.trim() == "") {
                    $("#validDocumentNo").html("This field is required.");
                    flag = false; card = false;
                }
                //if (CountryofIssue == null || CountryofIssue.trim() == "") {
                //    $("#validCountryofIssue").html("This field is required.");
                //    flag = false;
                //}

                if (DateOfIssue == null || DateOfIssue.trim() == "") {
                    $("#validDateOfIssue").html("This field is required.");
                    flag = false; card = false;
                }
                else if (isDate(DateOfIssue) == false) {
                    $("#validDateOfIssue").html("Enter Valid Date Of Issue.");
                    flag = false; card = false;
                }
                else if (parseInt(Today.replace(regExp, "$3$2$1")) < parseInt(DateOfIssue.replace(regExp, "$3$2$1"))) {   //if issue greater than today
                    $("#validDateOfIssue").html("Enter Valid Date Of Issue.");
                    flag = false; card = false;
                }

                if (ExpiryDate == null || ExpiryDate.trim() == "") {
                    $("#validExpiryDate").html("This field is required.");
                    flag = false; card = false;
                }
                else if (isDate(ExpiryDate) == false) {
                    $("#validExpiryDate").html("Enter Valid Expiry Date.");
